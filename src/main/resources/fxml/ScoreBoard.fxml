<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane style="-fx-background-color: #29292D" xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.RankingControl"
            prefHeight="600.0" prefWidth="800.0"
            stylesheets="@../css/score.css">
            <top>
                <HBox alignment="TOP_CENTER">
                    <Button text="back" onMouseClicked="#back"/>
                    <Button text="Ranking by Score" onMouseClicked="#byScore"/>
                    <Button text="Ranking by Difficulty" onMouseClicked="#byDifficulty"/>
                </HBox>
            </top>
</BorderPane>
