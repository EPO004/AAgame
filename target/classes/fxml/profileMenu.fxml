<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.ProfileControl"
            prefHeight="600.0" prefWidth="800.0"
            stylesheets="@../css/profile.css">
            <center>
                <VBox spacing="3" alignment="BOTTOM_CENTER">
                    <Button text="change username" onMouseClicked="#usernameChange"/>
                    <Button text="change password" onMouseClicked="#passwordChange"/>
                    <Button text="remove account" onMouseClicked="#removeAccount"/>
                    <Button text="logout" onMouseClicked="#logout"/>
                    <Button text="back" onMouseClicked="#back"/>
                </VBox>
            </center>
</BorderPane>
